  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "program": "${workspaceFolder}\\bin\\api-documenter",
      "args": [
        "html",
        "--input-folder",
        "${workspaceFolder}/sample-input",
        "--output-folder",
        "${workspaceFolder}/doc-output"
      ],
      "outFiles": [
        "${workspaceFolder}/lib/**/*.js"
      ],
      "sourceMaps": true,
      "preLaunchTask": "build"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch fast",
      "program": "${workspaceFolder}\\bin\\api-documenter",
      "args": [
        "html",
        "--input-folder",
        "${workspaceFolder}/sample-input",
        "--output-folder",
        "${workspaceFolder}/doc-output"
      ],
      "outFiles": [
        "${workspaceFolder}/lib/**/*.js"
      ],
      "sourceMaps": true
    }
  ]
}